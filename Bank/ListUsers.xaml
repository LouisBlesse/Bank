<Window x:Class="Bank.ListUsers"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:local="clr-namespace:Bank"
        mc:Ignorable="d"
        Title="ListUsers" Height="450" Width="800">
    
    <Grid>
        <Button Click="printUser" Content="Button" HorizontalAlignment="Left" Margin="346,44,0,0" VerticalAlignment="Top" Width="75" RenderTransformOrigin="-1.268,-7.296"/>
        <ScrollViewer HorizontalAlignment="Left" Height="292" Margin="36,89,0,0" VerticalAlignment="Top" Width="721" VerticalScrollBarVisibility="Auto">
            <ScrollViewer.Background>
                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                    <GradientStop Color="Black" Offset="0"/>
                    <GradientStop Color="#FF3F5293" Offset="1"/>
                </LinearGradientBrush>
            </ScrollViewer.Background>
            <StackPanel Name="StackUser"   />
        </ScrollViewer>
    </Grid>
</Window>
